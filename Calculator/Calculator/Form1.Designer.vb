    Private Sub ButtonEqual_Click(sender As Object, e As EventArgs) Handles ButtonEqual.Click
        TextBox1.Text += "="
        Console.WriteLine(TextBox1.Lines.ElementAt(0))
        Dim string1 As String = TextBox1.Lines.ElementAt(0)
        Dim Var1 As Integer = 0
        Dim Var2 As Integer = 0
        Dim OP As Char
        Dim Answer As Integer = 0
        Dim FirstVar As Boolean = True
        For Each character As Char In string1
            If IsNumeric(character) And FirstVar Then
                Var1 += Val(character)
                FirstVar = False
            End If
            If IsNumeric(character) And Not FirstVar Then
                Var2 += Val(character)
            End If
            If Not IsNumeric(character) And character <> "=" Then
                OP = character
                Console.WriteLine("Triggered not equal if statement")
            End If
            If Not IsNumeric(character) And character = "=" Then
                Select Case OP
                    Case "+"
                        Answer = Var1 + Var2
                    Case "-"
                        Answer = Var1 - Var2
                    Case "/"
                        Answer = Var1 / Var2
                    Case "*"
                        Answer = Var1 * Var2
                    Case "^"
                        Answer = Var1 ^ Var2
                End Select
                TextBox1.Text += Answer

            End If

        Next character

    End Sub
